  "dependencies": {
    "babel-cli": "6.5.x",
    "babel-core": "6.5.x",
    "babel-loader": "6.2.x",
    "babel-preset-es2015": "6.5.x",
    "babel-preset-react": "6.5.x",
    "babel-register": "^6.7.2",
